{
    "name": "Google Meet presence list",
    "description": "Automatically gives you a google meet presence list",
    "version": "0.0.0.1",
    "permissions": [
        "tabs","https://meet.google.com/*"
    ],
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons": { 
        "16": "icons/16.ico",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "64": "icons/64.png",
       "128": "icons/128.png" },
    "content_scripts": [
        {
        "matches": [
            "https://meet.google.com/*"
            ],
        "js": ["main.js"],
        "run_at": "document_end"
        }
    ],
    "web_accessible_resources":[
        "resources/*"
    ],
    "manifest_version": 2
}
